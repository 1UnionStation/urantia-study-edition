\newcommand{\quizpart}[1]{%
   \newpage
   \vspace*{\stretch{0.2}}%
   \ifnum #1=1
      \ubookdescr{qb1}{toc={\MakeUppercase{\partimix}},bookmark=\partimix,abbr={qp1}}%
      \begin{center}\titlefont\huge\MakeUppercase{\partimix}\end{center}%
   \fi
   \ifnum #1=2
      \ubookdescr{qb2}{toc={\MakeUppercase{\partiimix}},bookmark=\partiimix,abbr={qp2}}%
      \begin{center}\titlefont\huge\MakeUppercase{\partiimix}\end{center}%
   \fi
   \ifnum #1=3
      \ubookdescr{qb3}{toc={\MakeUppercase{\partiiimix}},bookmark=\partiiimix,abbr={qp3}}%
      \begin{center}\titlefont\huge\MakeUppercase{\partiiimix}\end{center}%
   \fi
   \ifnum #1=4
      \ubookdescr{qb4}{toc={\MakeUppercase{\partivmix}},bookmark=\partivmix,abbr={qp4}}%
      \begin{center}\titlefont\huge\MakeUppercase{\partivmix}\end{center}%
   \fi
   \vspace*{\stretch{0.7}}%
   \newpage
}

\newcounter{quizchapter}

\newcommand{\quizchap}[1]{%
   \newpage
   \stepcounter{quizchapter}%
   \vspace*{\stretch{0.2}}%
   \bib@raise@anchor{\bibpdfbookmark[2]{\thequizchapter. #1}{qc\thequizchapter}\label{qc\thequizchapter}\hypertarget{qc\thequizchapter}{}}%
   \begin{center}\titlefont\LARGE\thequizchapter.\nobreakspace\MakeUppercase{#1}\end{center}
   \vspace*{\stretch{0.7}}%
   \newpage
}

\newcounter{curq}
\newcounter{cans}

\newcommand{\printquestion}{\begin{center}\questionfont\question\end{center}}

\newcommand{\qstart}[2]{%
  \newpage
  \setcounter{cans}{0}%
  \stepcounter{curq}%
  \gdef\question{#1}%
  \ifx #2\relax\gdef\references{NONE}\else\gdef\references{#2}\fi
  \printquestion
  \begin{itemize}%
  \answersfont
  \@answers
}

\newcommand\@answers{\@ifnextchar\qstop{\@answersend}{\@nextanswer}}

\newcommand\@nextanswer[2]{%
  \stepcounter{cans}%
  \item \hyperlink{ans\thecurq}{#1}%
  \ifnum \value{cans}=1 \gdef\answerone{#1 \ifnum #2=1 \cmark\else\xmark\fi}\fi
  \ifnum \value{cans}=2 \gdef\answertwo{#1 \ifnum #2=1 \cmark\else\xmark\fi}\fi
  \ifnum \value{cans}=3 \gdef\answerthree{#1 \ifnum #2=1 \cmark\else\xmark\fi}\fi
  \ifnum \value{cans}=4 \gdef\answerfour{#1 \ifnum #2=1 \cmark\else\xmark\fi}\fi
  \ifnum \value{cans}=5 \gdef\answerfive{#1 \ifnum #2=1 \cmark\else\xmark\fi}\fi
  \ifnum \value{cans}=6 \gdef\answersix{#1 \ifnum #2=1 \cmark\else\xmark\fi}\fi
  \ifnum \value{cans}=7 \gdef\answerseven{#1 \ifnum #2=1 \cmark\else\xmark\fi}\fi
  \ifnum \value{cans}=8 \gdef\answereight{#1 \ifnum #2=1 \cmark\else\xmark\fi}\fi
  \ifnum \value{cans}=9 \gdef\answernine{#1 \ifnum #2=1 \cmark\else\xmark\fi}\fi
  \ifnum \value{cans}=10 \gdef\answerten{#1 \ifnum #2=1 \cmark\else\xmark\fi}\fi
  \@answers
}

\def\none{NONE}

\newcommand\@answersend[1]{%
  \end{itemize}
  \newpage
  \hypertarget{ans\thecurq}{}%
  \printquestion
  \begin{itemize}%
  \answersfont
  \item \answerone
  \ifnum \value{cans}>1 \item \answertwo\fi
  \ifnum \value{cans}>2 \item \answerthree\fi
  \ifnum \value{cans}>3 \item \answerfour\fi
  \ifnum \value{cans}>4 \item \answerfive\fi
  \ifnum \value{cans}>5 \item \answersix\fi
  \ifnum \value{cans}>6 \item \answerseven\fi
  \ifnum \value{cans}>7 \item \answereight\fi
  \ifnum \value{cans}>8 \item \answernine\fi
  \ifnum \value{cans}>9 \item \answerten\fi
  \end{itemize}%
  \ifx\references\none\else\vspace*{2ex}\hmark\ \references\fi
}

